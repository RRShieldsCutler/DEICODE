    "\n",
    "# filter sample to min depth\n",
    "def sample_filter(val, id_, md): return sum(val) > 500\n",
    "def observation_filter(val, id_, md): return sum(val) > 10\n",
    "table = table.filter(sample_filter, axis='sample')\n",
